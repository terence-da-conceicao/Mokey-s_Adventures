function player_movement(objet)

	newx=p.x
	newy=p.y
	if p.anim_t==0 then -- autorise le mouvement  la fin de l'animation de l'offset 
	newox=0 --valeur de base du mouvement de l'offset
	newoy=0
	
	if (btn(➡️)) then 
	 newx+=1
	 newox=-8 -- deplace notre offset de 8 pixel vers la gauche
	 p.flip=false
	 move_sprite(objet)
	 	
	
	elseif (btn(⬅️)) then
	 newx-=1
	 newox=8
	 p.flip=true
	 move_sprite(objet)
		
	elseif (btn(⬇️)) then 
	 newy+=1
	 newoy=-8
	 move_sprite(objet)
	
	
	elseif (btn(⬆️)) then 
	 newy-=1
	 newoy=8
	 move_sprite (objet) 
	end
	

	interact(newx,newy)
	
	if not check_flag(0,newx,newy)
	and (p.x!=newx or p.y!=newy) then -- permet de declarer le mouvement pour lancer lanimation
		p.x=mid(0,newx,127) 
		p.y=mid(0,newy,63)
		p.start_ox=newox --- donne l'autorisation a notre offset de bouger 
		p.start_oy=newoy
		p.anim_t=1 -- lance l'animation 
elseif check_flag(0,newx,newy) and check_flag(3,newx,newy)
		and attack_with_sword then	
			enemy=mget(newx,newy)
			mset(newx,newy,enemy+1)
	end
end
