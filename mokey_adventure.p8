	-- animation 
	p.anim_t=max(p.anim_t-0.25,0)-- 0.25 reprsente deux pixel par frame 2/8 = 0.25
		p.ox=p.start_ox*p.anim_t
		p.oy=p.start_oy*p.anim_t

end

