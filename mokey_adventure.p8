end

function check_flag(flag,x,y)
	local sprite=mget(x,y)
	return fget(sprite,flag)
end

	function update_camera()
 
end
	function fix_camera()
		camx=mid(0,p.x-7.5,31-15)
		camy=mid(0,p.y-7.5,31-15)
		camera(camx*8,camy*8)
	end
	
	function init_camera()
	camx,camy=0,0
end

function travel_camera()
	sectionx=flr(p.x/16)*16
	sectiony=flr(p.y/16)*16
	
	destx=sectionx*8
	desty=sectiony*8
	
	diffx=destx-camx
	diffy=desty-camy
	
	diffx/=4
	diffy/=4
	
	camx+=diffx
	camy+=diffy
	
	camera(camx,camy)
end
	
function next_tile(x,y)
	sprite=mget(x,y)
	mset(x,y,sprite+1)
end

function pick_up_key(x,y)
	next_tile(x,y)
	p.keys+=1
	sfx(0)
	create_msg("monkey",rnd({"une nouvelle cle","youpii","une cle en plus","je vais pouvoir sauver mes amis","encore une","j'ai trouve une cle !","genial!!!","merci pour ton aide erik","les amis j'arrive !!","je ne vous laisserais pas tomber !","j'aurais prefere une banane.","une nouvelle cle  !!","on va liberer tous nos amis !","super une cle !!","elle etait bien cache celle la",}))
end

function pick_up_friend(x,y)
	next_tile(x,y)
	p.friends+=1
	sfx(20)
	create_msg("ami",rnd({"merci","merci de m'avoir sauver","tu dead ca chacal","oh mais j'etais bien ici","non pas la d.a.s.s"}))
	end 

function open_door(x,y)
	next_tile(x,y)
	p.keys-=1
	sfx(1)
end

function bloukatchu(x,y)
	next_tile(x,y)
	p.keys-=1
	sfx(21)
	create_msg("bloukachu",rnd({"merci pour la cle","mmmh de l or","je te prend ca \ntu en a pas besoin","tu as trop de cle","tu en trouvera ailleur","pas de cle pour toi","tes amis resterons emprisone"}))
end 
function secret(x,y)
	next_tile(x,y)
	sfx(13)
end
	

	
